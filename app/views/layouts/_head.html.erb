<title><%= content_for(:title) || current_church %></title>
<%= csrf_meta_tags %>

<%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
<%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

<% if Setting.present?(:gmaps_api_key) && !Rails.env.test? %>
  <%= javascript_include_tag "//maps.googleapis.com/maps/api/js?key=#{Setting.fetch(:gmaps_api_key)}", 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'gmaps', 'data-turbolinks-track': 'reload' %>
<% end %>

<meta name="viewport" content="width=device-width, initial-scale=1" />
<link rel="icon" href="/favicon.png" />
